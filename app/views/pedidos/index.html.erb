<p id="notice"><%= notice %></p>

<h1>Pedidos</h1>

<table>
  <thead>
    <tr>
      <th>Hora pedido</th>
      <th>Hora entrega</th>
      <th>Observacion</th>
      <th>Registro</th>
      <th>Estado</th>
      <th>Creado por</th>
      <th>Actualizado por</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pedidos.each do |pedido| %>
      <tr>
        <td><%= pedido.hora_pedido %></td>
        <td><%= pedido.hora_entrega %></td>
        <td><%= pedido.observacion %></td>
        <td><%= pedido.registro_id %></td>
        <td><%= pedido.estado %></td>
        <td><%= pedido.creado_por %></td>
        <td><%= pedido.actualizado_por %></td>
        <td><%= link_to 'Show', pedido %></td>
        <td><%= link_to 'Edit', edit_pedido_path(pedido) %></td>
        <td><%= link_to 'Destroy', pedido, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pedido', new_pedido_path %>
