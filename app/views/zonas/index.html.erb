<p id="notice"><%= notice %></p>

<h1>Zonas</h1>

<table>
  <thead>
    <tr>
      <th>Zona</th>
      <th>Tarifa</th>
      <th>Estado</th>
      <th>Creado por</th>
      <th>Actualizado por</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @zonas.each do |zona| %>
      <tr>
        <td><%= zona.zona %></td>
        <td><%= zona.tarifa %></td>
        <td><%= zona.estado %></td>
        <td><%= zona.creado_por %></td>
        <td><%= zona.actualizado_por %></td>
        <td><%= link_to 'Show', zona %></td>
        <td><%= link_to 'Edit', edit_zona_path(zona) %></td>
        <td><%= link_to 'Destroy', zona, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Zona', new_zona_path %>
